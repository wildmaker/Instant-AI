{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/.next/**": true,
    "**/storage/**": true,
    "**/storage/documents/**": true,
    "**/docker/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": false
  },
  "cSpell.words": [
    "AIbitat",
    "allm",
    "instant-ai",
    "Apipie",
    "Astra",
    "autofetch",
    "autosave",
    "bedrock",
    "botml",
    "chatgpt",
    "classnames",
    "clickaway",
    "conv",
    "cosine",
    "curr",
    "datasets",
    "datetime",
    "devcontainer",
    "dont",
    "embeddings",
    "Errl",
    "Errno",
    "esbenp",
    "esmodule",
    "exsited",
    "femto",
    "filepath",
    "freqeuncypenalty",
    "gemini",
    "gemma",
    "groq",
    "Illeism",
    "jspdf",
    "langchain",
    "LANCEDB",
    "lstms",
    "Milvus",
    "Mintplex",
    "nextjs",
    "nhost",
    "noninteractive",
    "okta",
    "ollama",
    "openai",
    "openrouter",
    "PPLX",
    "prettierrc",
    "prisma",
    "qdrant",
    "SINGLESIGNON",
    "supabad",
    "Teensy",
    "unarchive",
    "unarchived",
    "unhighlight",
    "Vectorize",
    "weaviate",
    "webhooks",
    "Xenova",
    "Zilliz"
  ],
  "eslint.experimental.useFlatConfig": true,
  "docker.languageserver.formatter.ignoreMultilineInstructions": true,
  "editor.formatOnSave": true,
  "explorer.experimental.fileNesting.enabled": true,
  "explorer.experimental.fileNesting.expand": false,
  "explorer.experimental.fileNesting.patterns": {
    "*.js": "$(capture).js.map, $(capture).d.ts, $(capture).d.ts.map",
    "*.jsx": "$(capture).js, $(capture).jsx.map, $(capture).d.ts, $(capture).d.ts.map, $(capture).scss, $(capture).module.css, $(capture).module.scss, $(capture).css",
    "*.ts": "$(capture).js, $(capture).js.map, $(capture).d.ts, $(capture).d.ts.map",
    "*.tsx": "$(capture).ts, $(capture).ts.map, $(capture).d.ts, $(capture).d.ts.map, $(capture).scss, $(capture).module.css, $(capture).module.scss, $(capture).css",
    "*.vue": "$(capture).js, $(capture).jsx, $(capture).ts, $(capture).tsx, $(capture).hbs, $(capture).scss, $(capture).module.css, $(capture).module.scss, $(capture).css",
    "package.json": ".npmrc, package-lock.json, yarn.lock, .yarnrc, pnpm-lock.yaml",
    "tsconfig.json": "tsconfig.*.json"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "sarif-viewer.connectToGithubCodeScanning": "off"
}